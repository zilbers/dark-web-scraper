n  }\r\n\r\n  @media (min-width: 900px) {\r\n    width: 40%;\r\n  }\r\n\r\n  @media (min-width: 1600px) {\r\n    width: 600px;\r\n  }\r\n`;\r\n\r\nconst COLORS = [\r\n  '#0088FE',\r\n  '#00C49F',\r\n  '#FFBB28',\r\n  '#FF8042',\r\n  '#AAAAAA',\r\n  '#800080',\r\n];\r\n\r\nconst renderActiveShape = (props) => {\r\n  const RADIAN = Math.PI / 180;\r\n  const {\r\n    cx,\r\n    cy,\r\n    midAngle,\r\n    innerRadius,\r\n    outerRadius,\r\n    startAngle,\r\n    endAngle,\r\n    fill,\r\n    payload,\r\n    percent,\r\n    value,\r\n    label,\r\n  } = props;\r\n  const sin = Math.sin(-RADIAN * midAngle);\r\n  const cos = Math.cos(-RADIAN * midAngle);\r\n  const sx = cx + (outerRadius + 10) * cos;\r\n  const sy = cy + (outerRadius + 10) * sin;\r\n  const mx = cx + (outerRadius + 30) * cos;\r\n  const my = cy + (outerRadius + 30) * sin;\r\n  const ex = mx + (cos >= 0 ? 1 : -1) * 22;\r\n  const ey = my;\r\n  const textAnchor = cos >= 0 ? 'start' : 'end';\r\n\r\n  return (\r\n    <g key={payload.name}>\r\n      <text x={cx} y={cy} dy={8} textAnchor='middle' fill={fill}>\r\n        {payload.name}\r\n      </text>\r\n      <Sector\r\n        cx={cx}\r\n        cy={cy}\r\n        innerRadius={innerRadius}\r\n        outerRadius={outerRadius}\r\n        startAngle={startAngle}\r\n        endAngle={endAngle}\r\n        fill={COLORS[value % COLORS.length]}\r\n      />\r\n      <Sector\r\n        cx={cx}\r\n        cy={cy}\r\n        startAngle={startAngle}\r\n        endAngle={endAngle}\r\n        innerRadius={outerRadius + 6}\r\n        outerRadius={outerRadius + 10}\r\n        fill={COLORS[value % COLORS.length]}\r\n      />\r\n      <path\r\n        d={`M${sx},${sy}L${mx},${my}L${ex},${ey}`}\r\n        stroke={fill}\r\n        fill='none'\r\n      />\r\n      <circle cx={ex} cy={ey} r={2} fill={fill} stroke='none' />\r\n      <text\r\n        x={ex + (cos >= 0 ? 1 : -1) * 12}\r\n        y={ey}\r\n        textAnchor={textAnchor}\r\n        fill='#333'\r\n      >{`PV ${label}`}</text>\r\n      <text\r\n        x={ex + (cos >= 0 ? 1 : -1) * 12}\r\n        y={ey}\r\n        dy={18}\r\n        textAnchor={textAnchor}\r\n        fill='#999'\r\n      >\r\n        {`(Rate ${(percent * 100).toFixed(2)}%)`}\r\n      </text>\r\n    </g>\r\n  );\r\n};\r\n\r\nfunction Chart({ url, wordList }) {\r\n  const [data, setData] = useState([]);\r\n  const [ready, setReady] = useState(false);\r\n  const [showing, setShowing] = useState(() => {\r\n    const newShowing = {};\r\n    for (const key of wordList) {\r\n      newShowing[key] = false;\r\n    }\r\n    return newShowing;\r\n  });\r\n  const [activeIndex, setActiveIndex] = useState();\r\n\r\n  async function getAndSet(url, wordList, setter) {\r\n    const newData = [];\r\n    for (const word of wordList) {\r\n      if (showing[word]) {\r\n        const { data: result } = await axios.get(`${url}?q=${word}`);\r\n        newData.push(result);\r\n      }\r\n    }\r\n    setter(newData);\r\n  }\r\n\r\n  const onPieEnter = (data, index) => {\r\n    setActiveIndex(index);\r\n  };\r\n\r\n  useEffect(() => {\r\n    getAndSet(url, wordList, setData);\r\n    setReady(true);\r\n  }, [wordList, showing]);\r\n\r\n  return (\r\n    <>\r\n      <Container>\r\n        <h3>Keywords Appearance</h3>\r\n        <CheckBox\r\n          showing={showing}\r\n          setShowing={setShowing}\r\n          options={wordList}\r\n        />\r\n        {ready && (\r\n          <ResponsiveContainer width='100%' aspect={4.0 / 3.0} maxHeight={300}>\r\n            <PieChart width={730} height={250}>\r\n              <Pie\r\n                activeIndex={activeIndex}\r\n                activeShape={renderActiveShape}\r\n                data={data}\r\n                dataKey='value'\r\n                nameKey='word'\r\n                outerRadius={50}\r\n                fill='#8884d8'\r\n                onMouseEnter={onPieEnter}\r\n              ></Pie>\r\n            </PieChart>\r\n          </ResponsiveContainer>\r\n        )}\r\n      </Container>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Chart;\r\n","C:\\Users\\omri_\\Documents\\GitHub\\dark-web-scraper\\client\\src\\components\\CheckBox.js",[],"C:\\Users\\omri_\\Documents\\GitHub\\dark-web-scraper\\client\\src\\components\\Bins.js",["52","53","54","55"],"C:\\Users\\omri_\\Documents\\GitHub\\dark-web-scraper\\client\\src\\hooks\\useLogsSearch.js",[],{"ruleId":"56","severity":1,"message":"57","line":28,"column":10,"nodeType":"58","messageId":"59","endLine":28,"endColumn":16},{"ruleId":"56","severity":1,"message":"60","line":28,"column":18,"nodeType":"58","messageId":"59","endLine":28,"endColumn":27},{"ruleId":"56","severity":1,"message":"61","line":7,"column":8,"nodeType":"58","messageId":"59","endLine":7,"endColumn":17},{"ruleId":"56","severity":1,"message":"62","line":12,"column":8,"nodeType":"58","messageId":"59","endLine":12,"endColumn":18},{"ruleId":"56","severity":1,"message":"63","line":11,"column":18,"nodeType":"58","messageId":"59","endLine":11,"endColumn":21},{"ruleId":"64","severity":1,"message":"65","line":43,"column":6,"nodeType":"66","endLine":43,"endColumn":8,"suggestions":"67"},{"ruleId":"56","severity":1,"message":"68","line":7,"column":3,"nodeType":"58","messageId":"59","endLine":7,"endColumn":7},{"ruleId":"56","severity":1,"message":"69","line":8,"column":3,"nodeType":"58","messageId":"59","endLine":8,"endColumn":8},{"ruleId":"64","severity":1,"message":"70","line":146,"column":6,"nodeType":"66","endLine":146,"endColumn":25,"suggestions":"71"},{"ruleId":"56","severity":1,"message":"63","line":11,"column":18,"nodeType":"58","messageId":"59","endLine":11,"endColumn":21},{"ruleId":"56","severity":1,"message":"72","line":23,"column":7,"nodeType":"58","messageId":"59","endLine":23,"endColumn":17},{"ruleId":"56","severity":1,"message":"73","line":40,"column":7,"nodeType":"58","messageId":"59","endLine":40,"endColumn":21},{"ruleId":"56","severity":1,"message":"74","line":55,"column":16,"nodeType":"58","messageId":"59","endLine":5